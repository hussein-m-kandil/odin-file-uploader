{
  "name": "odin-file-uploader",
  "version": "1.0.0",
  "description": "\"A stripped-down version of personal storage service like Google Drive. Built as part of studying The Odin Project's Node.js course.\"",
  "main": "api/app.js",
  "scripts": {
    "start": "node api/app.js",
    "dev": "node --env-file=.env --watch api/app.js",
    "lint": "eslint .",
    "vercel:dev": "[ -f .env.vercel ] && export $(cat .env.vercel) && vercel dev --listen 3000",
    "vercel:deploy": "vercel",
    "vercel:production": "vercel --prod",
    "postinstall": "prisma generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hussein-m-kandil/odin-file-uploader.git"
  },
  "keywords": [
    "node",
    "express",
    "passport",
    "express-session",
    "ejs",
    "prisma",
    "postgresql",
    "web-development",
    "backend",
    "bcrypt"
  ],
  "author": "\"Hussein Kandil\"",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hussein-m-kandil/odin-file-uploader/issues"
  },
  "homepage": "https://github.com/hussein-m-kandil/odin-file-uploader#readme",
  "prisma": {
    "schema": "./api/src/model/schema.prisma"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@stylistic/eslint-plugin-js": "^2.12.0",
    "eslint": "^9.16.0",
    "globals": "^15.13.0",
    "prisma": "^6.1.0",
    "vercel": "^39.2.2"
  },
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "@quixo3/prisma-session-store": "^3.1.13",
    "@supabase/supabase-js": "^2.47.9",
    "bcryptjs": "^2.4.3",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0"
  }
}
